{
    "grammarly.selectors": [
        {
            "language": "markdown",
            "scheme": "file"
        }
    ],
    "cSpell.words": [
        "EV's",
        "Fivetran",
        "Hudi",
        "Influencers",
        "lakehouse",
        "solopreneurship"
    ],
    "markdown.copyFiles.destination": {
        "/src/content/blog/*": "/public/posts/${documentBaseName}/"
    }
}